<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.uengine.web.process.ProcessMapRepository">

    <resultMap id="ProcessMap" type="org.uengine.web.process.ProcessMap">
        <result property="mapId" column="mapId" jdbcType="VARCHAR"/>
        <result property="defId" column="defId" jdbcType="VARCHAR"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="iconPath" column="iconPath" jdbcType="VARCHAR"/>
        <result property="color" column="color" jdbcType="VARCHAR"/>
        <result property="comCode" column="comcode" jdbcType="VARCHAR"/>
        <result property="no" column="no" jdbcType="INTEGER"/>
        <result property="cmPhrase" column="cmphrase" jdbcType="VARCHAR"/>
        <result property="cmTrgr" column="cmtrgr" jdbcType="VARCHAR"/>
        <result property="isScheduled" column="isScheduled" jdbcType="BOOLEAN"/>
    </resultMap>

    <insert id="insert" parameterType="org.uengine.web.process.ProcessMap"
            useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        INSERT INTO processmap
        (
        mapId,
        defId,
        name,
        iconPath,
        color,
        comcode,
        no,
        cmphrase,
        cmtrgr,
        isScheduled
        )
        VALUES (
          #{mapId},
          #{defId},
          #{name},
          #{iconPath},
          #{color},
          #{comCode},
          #{no},
          #{cmPhrase},
          #{cmTrgr},
          #{isScheduled}
        )
    </insert>

    <select id="selectAll" resultMap="ProcessMap">
        SELECT
        *
        FROM
        processmap
        ORDER BY name
    </select>

    <select id="selectByMapId" parameterType="string" resultMap="ProcessMap">
        SELECT
        *
        FROM
        processmap
        WHERE mapId = #{mapId}
        ORDER BY name
    </select>
</mapper>